{
  "presets": [            //设定转码规则
    ["env", {             
      "modules": false,   
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "es2015",
    "stage-2"
  ],
  "plugins": ["transform-runtime"],
  "env": {                      //设定特定环境下的特定选项
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  } 
}